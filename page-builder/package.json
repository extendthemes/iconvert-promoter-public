  "private": true,
  "scripts": {
    "dev-free": "npm run clean && npm run build:package-types && npm run dev:lite-free",
    "dev:lite-free": "npm run build:packages-free -- --internal=true && npm run dev:webpack-free",
    "build-free": "npm run clean && npm run build:packages-free  && npm run wp:build-free",
    "clean": "concurrently -r \"npm run clean:package-types\" \"npm run clean:cache\" \"npm run clean:packages\"",
    "build:packages-free": "node ./bin/packages/build.js --free=true",
    "dev:packages-free": "node ./bin/packages/watch.js \"--free\" \"--internal\"",
    "dev:webpack-free": "concurrently -r \"npm run wp:start-free\" \"npm run dev:packages-free\"",
    "clean:packages": "rimraf \"./src/packages/*/@(build|build-module|build-style|node_modules)\"",
    "clean:cache": "rimraf \"./node_modules/.cache/*\"",
    "clean:package-types": "tsc --build --clean",
    "wp:build-free": "wp-scripts build --env=production --env=free",
    "format-js": "wp-scripts format",
    "format-php": "composer run-script format",
    "lint-php": "composer run-script lint",
    "php-compatibility-check": "composer run-script compatibility-check",
    "lint-js": "wp-scripts lint-js",
    "lint-js:fix": "npm run lint-js -- --fix",
    "generate-svg-allowed-elements": "node ./bin/generate-svg-allowed-elements.js",
    "sync-package-json": "node ./bin/packages/sync-package-json-imports.js",
    "update-packages-and-sync": " npm run packages-update && npm run sync-package-json",
    "lerna:init": "./node_modules/.bin/lerna init",
    "lerna:clean": "./node_modules/.bin/lerna clean",
    "lerna:link": "./node_modules/.bin/lerna link",
    "make-pot": "node ./bin/make-pot.js"
  },
